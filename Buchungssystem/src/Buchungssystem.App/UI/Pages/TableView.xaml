<Page
    x:Class="Buchungssystem.App.UI.Pages.TableView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Buchungssystem.App.UI.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModel="clr-namespace:Buchungssystem.App.ViewModel"
    Title="TableView"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">

    <Grid>
        <DockPanel>
            <StackPanel DockPanel.Dock="Bottom">
                <TextBlock Text="{Binding Path=VerbleibenderPreis}" />
            </StackPanel>
        </DockPanel>
    </Grid>
</Page>
